{
  "root": true,
  "extends": [
    "plugin:import/recommended",
    "airbnb-typescript/base",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2022,
      "project": "./tsconfig.json"
  },
  "env": {
    "es2021": true
  },
  "rules": {
    "import/no-commonjs": "error"
  }
}
